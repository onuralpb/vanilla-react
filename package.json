{
	"name": "vanilla-react",
	"version": "1.0.0",
	"description": "React, Babel and Webpack without create-react-app",
	"main": "index.js",
	"scripts": {
		"dev": "webpack --mode development",
		"jserver": "json-server --watch db.json --port 5555",
		"build": "webpack --mode production",
		"addJson": "webpack --profile --json",
		"addOutFile": "Out-file 'dist/stats.json' -Encoding OEM",
		"reports": " webpack-bundle-analyzer dist/stats.json -m static -r dist/report.html",
		"serve": "webpack-dev-server --compress",
		"analyze": "source-map-explorer dist/*.js",
		"addStats": "concurrently \"npm:build\" \"npm:addJson\" \"npm:addOutFile\" \"npm:reports\"",
		"start": "concurrently \"npm:dev\" \"npm:jserver\" \"npm:serve\""
	},
	"author": "Onur Alp Baklaci",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "7.6.2",
		"@babel/core": "7.6.2",
		"@babel/node": "7.6.2",
		"@babel/plugin-proposal-class-properties": "7.5.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.6.2",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "7.6.2",
		"@babel/preset-react": "7.0.0",
		"@chenfengyuan/datepicker": "1.0.9",
		"axios": "^0.19.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "8.0.6",
		"classnames": "2.2.6",
		"compression-webpack-plugin": "^3.0.0",
		"create-react-context": "0.3.0",
		"css-loader": "3.2.0",
		"eslint-plugin-react-hooks": "2.0.1",
		"file-loader": "4.2.0",
		"html-loader": "0.5.5",
		"html-webpack-plugin": "3.2.0",
		"jquery": "^3.4.1",
		"jquery-countdown": "2.2.0",
		"json-server": "0.15.1",
		"loaders.css": "0.1.2",
		"node-sass": "4.13.1",
		"path": "0.12.7",
		"react-bootstrap": "1.0.0-beta.12",
		"react-code-splitting": "^1.2.1",
		"react-helmet": "5.2.1",
		"react-hot-loader": "^4.12.14",
		"react-loadable": "^5.5.0",
		"react-loaders": "3.0.1",
		"react-redux": "7.1.1",
		"react-router-dom": "5.1.0",
		"react-scripts": "3.1.2",
		"react-toastify": "5.4.0",
		"redux": "4.0.4",
		"redux-logger": "3.0.6",
		"redux-thunk": "2.3.0",
		"sass-loader": "8.0.0",
		"select2": "4.0.10",
		"source-map-explorer": "2.1.0",
		"style-loader": "1.0.0",
		"webpack": "4.41.0",
		"webpack-bundle-analyzer": "^3.5.2",
		"webpack-cli": "3.3.9",
		"webpack-dev-server": "3.8.1",
		"webpack-monitor": "^1.0.14"
	},
	"dependencies": {
		"@babel/runtime": "^7.6.2",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"eslint": "^6.4.0",
		"react": "16.9.0",
		"react-dom": "16.9.0",
		"windows-build-tools": "^5.2.2"
	},
	"eslintConfig": {
		"extends": "react-app",
		"plugins": [
			"react-hooks"
		],
		"rules": {
			"react-hooks/rules-of-hooks": "error"
		}
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"bit": {
		"env": {
			"compiler": "bit.envs/compilers/react@1.0.2"
		},
		"componentsDefaultDirectory": "components/{name}",
		"packageManager": "npm"
	}
}
